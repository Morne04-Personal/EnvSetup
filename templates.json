{
  "version": "2",
  "templates": [
    {
      "type": 1,
      "name": "radarr:latest",
      "image": "lscr.io/linuxserver/radarr:latest",
      "description": "This is a Portainer template for deploying radarr:latest.",
      "categories": [
        "Media"
      ],
      "restart_policy": "unless-stopped",
      "env": [
        {
          "name": "TZ",
          "default": "Africa/Johannesburg"
        },
        {
          "name": "PUID",
          "default": "0"
        },
        {
          "name": "PGID",
          "default": "0"
        },
        {
          "name": "PS1",
          "default": "$(whoami)@$(hostname):$(pwd)\\$"
        },
        {
          "name": "PATH",
          "default": "/lsiopy/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
        },
        {
          "name": "HOME",
          "default": "/root"
        },
        {
          "name": "TERM",
          "default": "xterm"
        },
        {
          "name": "S6_CMD_WAIT_FOR_SERVICES_MAXTIME",
          "default": "0"
        },
        {
          "name": "S6_VERBOSITY",
          "default": "1"
        },
        {
          "name": "S6_STAGE2_HOOK",
          "default": "/docker-mods"
        },
        {
          "name": "VIRTUAL_ENV",
          "default": "/lsiopy"
        },
        {
          "name": "LSIO_FIRST_PARTY",
          "default": "true"
        },
        {
          "name": "XDG_CONFIG_HOME",
          "default": "/config/xdg"
        }
      ],
      "volumes": [
        {
          "bind": "${/downloads/movies}",
          "container": "/movies"
        },
        {
          "bind": "${/containers/radarr/config}",
          "container": "/config"
        },
        {
          "bind": "${/downloads}",
          "container": "/downloads"
        }
      ],
      "ports": [
        "${1001}:7878"
      ]
    },
